     <div class = "row">
          <div class="col-sm-3 dimension-title results-margin"><%=dimension.LabelLeft%></div>
          <div class="col-sm-6 results-margin">
              <div class="progress <%="bg-purple" if dimension.lean == dimension.LabelRight%>">
                  <div class="progress-bar <%= dimension.lean == dimension.LabelRight ? "bg-grey":"bg-purple" %>" role="progressbar" style="width: 50%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
          </div>
          <div class="col-sm-3 dimension-title results-margin"><%=dimension.LabelRight%></div>
      </div>